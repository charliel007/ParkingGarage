@model VehicleDetail

@{
    ViewData["Title"] = Model.LicensePlateNumber;
}

<h1>License Plate: @Model.LicensePlateNumber</h1>
<h4>Vehicle Details</h4>
<hr />

<dl class="row">
    <dt class="col-sm-2">
        @Html.DisplayNameFor(model=>model.Year)
    </dt>
    <dd class="col-sm-10">
        @Html.DisplayFor(model=>model.Year)
    </dd>
    <dt class="col-sm-2">
        @Html.DisplayNameFor(model=>model.Color)
    </dt>
    <dd class="col-sm-10">
        @Html.DisplayFor(model=>model.Color)
    </dd>
    <dt class="col-sm-2">
        @Html.DisplayNameFor(model=>model.Make)
    </dt>
    <dd class="col-sm-10">
        @Html.DisplayFor(model=>model.Make)
    </dd>
    <dt class="col-sm-2">
        @Html.DisplayNameFor(model=>model.Model)
    </dt>
    <dd class="col-sm-10">
        @Html.DisplayFor(model=>model.Model)
    </dd>

    <tbody>
        @foreach (var VL in Model.VehicleLocations)
        {
            <tr>
                <td>@VL.Id</td>
                <td>@VL.VehicleEntityId</td>
                <td>@VL.LocationEntityId</td>


@*                <td>
                    <a asp-action="Details" asp-route-id="@vehicle.Id">Details</a> |
                    <a asp-action="Edit" asp-route-id="@vehicle.Id">Edit</a> |
                    <a asp-action="Delte" asp-route-id="@vehicle.Id">Delete</a>
                </td>*@
            </tr>
        }
    </tbody>


</dl>

<div>
    <a asp-action="Edit" asp-route-id=@Model.Id>Edit</a> |
    <a asp-action="Index">Back To List</a>
</div>
